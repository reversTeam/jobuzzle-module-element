#import (Config.path +'/Kernel/Template/View/MasterView') = MasterView;

class ElementDataTemplateMiniatureView extends MasterView {

	initialState : function () {
		return {
			entity : this._template._entity
		};
	}

	update : function () {
		this.setState({ entity : this._template._entity });
	}

	render : function () {
		return (
			<div>{this.state.entity.getId() +' : '+ this.state.entity.getType()}</div>
		);
	}

}

#export ElementDataTemplateMiniatureView;