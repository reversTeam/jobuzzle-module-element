class ElementDataMiniatureTemplate extends ContentTemplate {

	_viewClassName = 'ElementDataTemplateMiniatureView';
	_entity = undefined;

	#default get, set for _entity;

	onListen : function () {
		var dispatcher = _super();

		return dispatcher;
	}

	update : function () {
		if (this.getVisibility())
			this._view.update();
	}

}